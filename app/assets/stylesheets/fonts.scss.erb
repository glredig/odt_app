@font-face {
    font-family: 'AlexBrushRegular';
    src: url('<%= asset_path("AlexBrush-Regular-OTF-webfont.eot") %>');
    src: url('<%= asset_path("AlexBrush-Regular-OTF-webfont.eot?#iefix") %>') format('embedded-opentype'),
         url('<%= asset_path("AlexBrush-Regular-OTF-webfont.woff") %>') format('woff'),
         url('<%= asset_path("AlexBrush-Regular-OTF-webfont.ttf") %>') format('truetype'),
         url('<%= asset_path("AlexBrush-Regular-OTF-webfont.svg#AlexBrushRegular") %>') format('svg');
    font-weight: normal;
    font-style: normal;
}